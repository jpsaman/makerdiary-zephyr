if BOARD_NRF52832_MDK

config BOARD
	default "nrf52832_mdk"

if UART_NRFX

config UART_0_NRF_TX_PIN
	default 20

config UART_0_NRF_RX_PIN
	default 19

config UART_0_NRF_RTS_PIN
	default 5

config UART_0_NRF_CTS_PIN
	default 7

endif # UART_NRFX

endif # BOARD_NRF52832_MDK
